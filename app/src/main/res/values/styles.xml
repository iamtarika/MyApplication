<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="tv_test">
        <item name="android:text">@color/tv_test</item>
    </style>
    <style name="tv_hello_world">
        <item name="android:textColor">@color/tv_hello_world</item>
        <item name="android:textSize">@dimen/tv_hello_world</item>
        <item name="android:textAllCaps">false</item>

    </style>
    <style name="BrandedSwitch" parent="AppTheme">
        <item name="colorAccent">#39ac39</item>
    </style>
</resources>
